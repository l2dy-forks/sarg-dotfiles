[user]
    name = Sergey Trofimov
    email = sarg@sarg.org.ru

[color]
    ui = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan

[alias]
    ci = commit
    co = checkout
    p = pull
    s = status
    st = status
    b = branch
    d = diff
    a = add
    l = log --pretty=oneline
[push]
	default = simple
